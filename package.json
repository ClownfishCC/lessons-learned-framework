{
  "name": "lessons-learned-framework",
  "version": "0.0.1",
  "description": "Capture, store and share your learned lessons in a simple to use web application.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./public ./.cache ./dist",
    "start": "concurrently \"npm run serve\" \"parcel index.html\"",
    "build:domain": "npm run clean && parcel build index.html --out-dir ./public",
    "link": "npm link mithril-ui-form",
    "serve": "rest-easy-loki"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DRIVER-EU/lessons-learned-framework.git"
  },
  "keywords": [
    "SPA",
    "lessons",
    "learned",
    "framework",
    "REST",
    "database",
    "lokijs",
    "dynamic",
    "forms"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DRIVER-EU/lessons-learned-framework/issues"
  },
  "homepage": "https://github.com/DRIVER-EU/lessons-learned-framework#readme",
  "dependencies": {
    "leaflet": "^1.5.1",
    "leaflet-draw": "^1.0.4",
    "material-icons": "^0.3.1",
    "materialize-css": "^1.0.0",
    "mithril": "^2.0.4",
    "mithril-leaflet": "^0.1.6",
    "mithril-materialized": "^0.14.9",
    "mithril-ui-form": "^0.3.5",
    "rest-easy-loki": "^0.3.2"
  },
  "devDependencies": {
    "@types/lokijs": "^1.5.2",
    "@types/materialize-css": "^1.0.6",
    "@types/mithril": "^2.0.0",
    "@types/leaflet": "^1.5.1",
    "autoprefixer": "^9.6.1",
    "concurrently": "^4.1.2",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.2"
  }
}
